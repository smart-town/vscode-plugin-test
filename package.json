{
    "activationEvents": [
        "onCommand:myplugin.entryCommand",
        "onView:database",
        "onView:testView"
    ],
    "contributes": {
        "commands":[
            {
                "command":"myextension.dbtest2",
                "title": "dbtest2",
                "icon":"./media/mysql.svg"
            },
            {
                "command":"myextension.database1",
                "title": "database1",
                "icon":"./media/database.svg"
            },
            {
                "command":"myplugin.entryCommand",
                "title": "entryCommand"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "cherryContainer",
                    "title": "cherry",
                    "icon": "./media/cherry.svg"
                }
            ]
        },
        "views": {
            "cherryContainer": [
                {
                    "id": "database",
                    "name": "database"
                },
                {
                    "id": "testView",
                    "name": "testView"
                },
                {
                    "id": "<(￣ˇ￣)/",
                    "name": "<(￣ˇ￣)/"
                }
                
            ]
        },
        "menus":{
            "view/title": [
                {
                    "command": "myextension.dbtest2",
                    "when": "view == database",
                    "group": "navigation"
                },
                {
                    "command": "myextension.database1",
                    "when": "view == database",
                    "group": "navigation"
                }
            ]
        }
    },
    
    "name": "plugintest",
    "version": "1.0.0",
    "description": "test plugin",
    "main": "./dist/extension.js",
    "scripts": {
        "test": "echo \"Error: no test specified\""
    },
    "engines": {
        "vscode": "^1.33.0"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^10.12.21",
        "ts-loader": "^5.4.4",
        "tslint": "^5.12.1",
        "typescript": "^3.3.1",
        "vscode": "^1.1.28",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.1"
    },
    "dependencies": {
        "@types/mysql": "^2.15.6",
        "mysql": "^2.17.1"
    },
    "keywords": [
        "vscode"
    ],
    "author": "luhha",
